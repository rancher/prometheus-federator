{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"misspell",
			"ineffassign",
			"gofmt"
		]
	},
	"run": {
		"timeout": "5m"
	},
	"issues": {
		"exclude-rules": [
			{
				"linters": ["revive"],
				"text": "should have comment or be unexported"
			},
			{
				"path": "_test.go",
				"linters": ["revive"]
			}
		],
		"exclude-files": [
			"/zz_generated_*"
		]
	}
}